# screensaver.properties.web.test
#
# to be deployed to Tomcat when running JSFUnit tests


### build-time properties

test-ui=true
servlet.authentication.method=BASIC
appserver.dir=/usr/local/tomcat
log4j.properties.file=cfg/log4j.properties.debug

### run-time properties

screensaver.ui.application_name=Screensaver
screensaver.ui.version=Test
screensaver.ui.feedback_url=mailto:screensaver-feedback@hms.harvard.edu

babel.executable.path=/usr/local/openbabel/bin/babel

# database connection
SCREENSAVER_PGSQL_SERVER=localhost
SCREENSAVER_PGSQL_DB=testscreensaver
SCREENSAVER_PGSQL_USER=testscreensaver
SCREENSAVER_PGSQL_PASSWORD=