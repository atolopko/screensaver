# screensaver.properties for junit tests


### build-time properties

log4j.properties.file=cfg/log4j.properties.debug
debug=true
test=true


### run-time properties
screensaver.service.smtpemailservice.host=smtp.mailinator.com
screensaver.service.smtpemailservice.username=

### cellHTS2 properties
cellHTS2report.filepath.base=cellHTS2/output/
cellHTS2report.filepath.prefix=cellHts2_output_screenResultId_
cellHTS2report.saveRObjects=FALSE
cellHTS2report.saveRObjects.path=


# database connection
SCREENSAVER_PGSQL_SERVER=localhost
SCREENSAVER_PGSQL_DB=testscreensaver
SCREENSAVER_PGSQL_USER=testscreensaver
SCREENSAVER_PGSQL_PASSWORD=