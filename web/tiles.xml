<!--
  One definition per view/page.
  "name" attribute of definition tag is inferred from the requested *.jsp URL.
  "put" elements define the parameters that are used to generate an instance of the referenced template.
-->


<tiles-definitions>

  <!-- "abstract" definitions -->

  <definition name=".base" path="/template.jsp">
    <put name="pageTitle" type="string" value="Screensaver" />
    <put name="pageHelpURL" value="" />
    <put name="menu" value="/menu.jsp" />
    <put name="header" value="/header.jsp" />
    <put name="footer" value="/footer.jsp" />
    <!-- the JSF ID of the input component that should have initial focus
         (specify forceId="true" in the JSF component definition) -->
    <put name="inputFocusId" value="" />
  </definition>

  <definition name=".unauthenticatedBase" extends=".base" path="/template.jsp">
    <put name="menu" value="menu-for-unauthenticated.jsp" />
    <put name="footer" value="/footer.jsp" />
  </definition>

  <definition name=".pageHelpBase" path="/help/pageHelpTemplate.jsp">
    <put name="pageTitle" type="string" value="" />
  </definition>



  <!-- Concrete definitions -->


  <!-- top-level tiles -->

  <definition name="/reportException.tiles" extends=".base">
    <put name="pageTitle" value="Screensaver Error!" />
    <put name="body" value="/reportException.jsp" />
  </definition>

  <definition name="/login.tiles" extends=".unauthenticatedBase">
    <put name="pageTitle" value="Screensaver kindly requests that you identify yourself as an authorized user." />
    <put name="body" value="/login.jsp" />
    <put name="inputFocusId" value="j_username" />
  </definition>

  <definition name="/goodbye.tiles" extends=".unauthenticatedBase">
    <put name="pageTitle" value="Thanks for using Screensaver!" />
    <put name="body" value="/goodbye.jsp" />
  </definition>

  <definition name="/login-error.tiles" extends=".unauthenticatedBase">
    <put name="pageTitle" value="Login failed! (sorry)" />
    <put name="body" value="/login-error.jsp" />
  </definition>

  <definition name="/main.tiles" extends=".base">
    <put name="pageTitle" value="" />
