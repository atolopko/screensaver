<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
 
  <bean 
    id="screenResultParser"
    class="edu.harvard.med.screensaver.io.screenresult.ScreenResultParser"
    singleton="true">
    <constructor-arg index="0">
      <bean class="edu.harvard.med.screensaver.io.screenresult.MockDaoForScreenResultParserTest"/>
    </constructor-arg>
    <!-- constructor-arg ref="dao"/-->
  </bean>
  
  <bean 
    id="screenDBDataLoader"
    class="edu.harvard.med.screensaver.db.screendb.ScreenDBDataLoader"
    singleton="true"
  >
    <constructor-arg ref="dao"/>
    <constructor-arg ref="schemaUtil"/>
  </bean>

</beans>
